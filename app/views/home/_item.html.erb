<li class="span3 clearfix">

  <div class="box style-two" data-hipop="two">

    <img src="assets/img/212x192.jpg" title="" alt="Img preview">


    <%= link_to '', item_action_path(item, "like"), :remote => true, :class => "popup" %>
    <%= link_to '', item_action_path(item, "own"), :remote => true, :class => "popup2" %>
    <%= link_to '', item_action_path(item, "want"), :remote => true, :class => "popup3" %>

  </div>

  <div class="thumbSetting">
    <div class="thumbTitle">
      <h3>
        <a href="#" class="invarseColor"><%=item.title%></a>
      </h3>
    </div>
    <ul class="rating clearfix">
      <li><i class="star-on"></i></li>
      <li><i class="star-on"></i></li>
      <li><i class="star-on"></i></li>
      <li><i class="star-on"></i></li>
      <li><i class="star-off"></i></li>
    </ul>
    <div class="product-desc">
      <p>
        Praesent ac condimentum felis. Nulla at nisl orci, at dignissim dolor...
      </p>
    </div>

    <div class="thumbButtons">
      <button class="btn btn-primary btn-small btn-block">
        Details
      </button>
    </div>

  </div>
</li>